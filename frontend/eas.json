{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "API_URL_DEV": "http://192.168.2.19:8001",
        "API_URL_PROD": "https://raam-finance-app.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "ENABLE_PROGUARD": "true",
          "ENABLE_SHRINK_RESOURCES": "true"
        }
      },
      "env": {
        "NODE_ENV": "production",
        "API_URL_DEV": "https://raam-finance-app.onrender.com",
        "API_URL_PROD": "https://raam-finance-app.onrender.com",
        "EXPO_PUBLIC_OPTIMIZE_BUNDLE": "true"
      },
      "cache": {
        "key": "preview-{{ platform }}-{{ hash }}"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "ENABLE_PROGUARD": "true",
          "ENABLE_SHRINK_RESOURCES": "true"
        }
      },
      "env": {
        "NODE_ENV": "production",
        "API_URL_DEV": "https://raam-finance-app.onrender.com",
        "API_URL_PROD": "https://raam-finance-app.onrender.com",
        "EXPO_PUBLIC_OPTIMIZE_BUNDLE": "true"
      },
      "cache": {
        "key": "production-{{ platform }}-{{ hash }}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
